@prefix : <https://w3id.org/elixir-code/test#> .
@prefix struct: <https://w3id.org/elixir-code/structure#> .
@prefix otp: <https://w3id.org/elixir-code/otp#> .
@prefix core: <https://w3id.org/elixir-code/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Valid GenServer implementation
# Tests:
# - GenServerImplementationShape: qualified constraint for init/1 callback
# - GenServerImplementationShape: sh:qualifiedMinCount 1

:server_module a struct:Module ;
    struct:moduleName "MyApp.Server"^^xsd:string .

:server_impl a otp:GenServerImplementation ;
    otp:implementsModule :server_module ;
    otp:hasGenServerCallback :server_init .

:server_init a otp:InitCallback , struct:Function ;
    struct:functionName "init"^^xsd:string ;
    struct:arity "1"^^xsd:nonNegativeInteger ;
    struct:belongsTo :server_module ;
    struct:hasClause :server_init_clause1 .

:server_init_clause1 a struct:FunctionClause ;
    struct:clauseOrder "1"^^xsd:positiveInteger ;
    struct:hasHead [
        a struct:FunctionHead ;
        core:hasParameter [
            a core:Parameter ;
            core:parameterPosition "1"^^xsd:positiveInteger ;
            core:parameterName "args"^^xsd:string
        ]
    ] ;
    struct:hasBody [
        a struct:FunctionBody
    ] .
